{
  "lock": false,
  "tasks": {
    "start": "deno run -A --unstable main.ts"
  },

  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.1/",
    "gfm": "https://deno.land/x/gfm@0.2.5/mod.ts",
    "kv_toolbox/": "https://deno.land/x/kv_toolbox@0.0.6/",
    "preact": "https://esm.sh/preact@10.19.2",
    "preact/": "https://esm.sh/preact@10.19.2/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.2",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.1",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.0",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/",
    "$std/": "https://deno.land/std@0.209.0/",
    "$components/": "https://deno.land/x/kview@0.6.0/components/",
    "$islands/": "https://deno.land/x/kview@0.6.0/islands/",
    "$utils/": "https://deno.land/x/kview@0.6.0/utils/",
    "./islands/": "https://deno.land/x/kview@0.6.0/islands/",
    "./routes/": "https://deno.land/x/kview@0.6.0/routes/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "exclude": [
    "**/_fresh/*"
  ]
}
